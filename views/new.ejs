<div class="container">
    
   
    <form method="POST" action="/blogs" >
      <div class="form-group">
        <label for="usr">Title</label>
        <input type="text" class="form-control" id="usr" name ="title" placeholder="Title">
      </div>
      <div class="form-group">
        <label for="pwd">Image url</label>
        <input type="text" class="form-control" id="pwd" name ="image" placeholder="Image url">
      </div>

      <div class="form-group">
        <label for="comment">Comment:</label>
        <textarea class="form-control" rows="5" id="comment" name="body"></textarea>
      </div>
      <div class="form-group">
        <button type="submit"  value="Submit">Submit</button>              
    </div>
    </form>
  </div>
  

